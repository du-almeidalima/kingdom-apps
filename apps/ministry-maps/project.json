{
  "projectType": "application",
  "sourceRoot": "apps/ministry-maps/src",
  "prefix": "kingdom-apps",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/ministry-maps",
        "index": "apps/ministry-maps/src/index.html",
        "main": "apps/ministry-maps/src/main.ts",
        "polyfills": "apps/ministry-maps/src/polyfills.ts",
        "tsConfig": "apps/ministry-maps/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ministry-maps/src/favicon.ico",
          "apps/ministry-maps/src/assets"
        ],
        "styles": [
          "apps/ministry-maps/src/styles/main.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/ministry-maps/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "ministry-maps:build:production"
        },
        "development": {
          "browserTarget": "ministry-maps:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ministry-maps:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/ministry-maps/**/*.ts",
          "apps/ministry-maps/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/ministry-maps"
      ],
      "options": {
        "jestConfig": "apps/ministry-maps/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {
        "prerender": false,
        "ssr": false,
        "browserTarget": "ministry-maps:build:production",
        "firebaseProject": "du-ministry-maps",
        "firebaseHostingSite": "du-ministry-maps"
      }
    }
  },
  "tags": []
}
